<!DOCTYPE html>
<html>
<head>
  <title>MyRecipes</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <head class="navbar navbar-fixed-top navbar-default">
    <div class="container">
      <%= link_to "myRecipes", "", class: "btn btn-lg btn-success", id: "logo" %>
      <div class="nav-item">
        <%= link_to "firstLink", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav-item">
        <%= link_to "secondLink", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav-item">
        <%= link_to "thirdLink", "", class: "btn btn-default btn-warning" %>
      </div>
    </div>
  </head>
  <div class="container">
    <%= yield %>
  </div>


</body>
</html>
